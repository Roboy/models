<sdf version="1.5">
    <model name="plate_with_muscle">
        <link name="plate">
            <pose>0 0 0 0 0 0</pose>
            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>100.0</mass>
                <inertia>
                    <ixx>10.0</ixx>
                    <ixy>0.0</ixy>
                    <ixz>0.0</ixz>
                    <iyy>10.0</iyy>
                    <iyz>0.0</iyz>
                    <izz>10.0</izz>
                </inertia>
            </inertial>
            <collision name="plate_collision">
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate_with_muscle/cad/plate.STL</uri>
                    </mesh>
                </geometry>
            </collision>
            <visual name="plate_visual">
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate_with_muscle/cad/plate.STL</uri>
                    </mesh>
                </geometry>
            </visual>
        </link>

        <link name="sphere">
            <pose>0 -0.06 0.505 -3.14159 -3.6656e-16 1.5708</pose>
            <inertial>
                <pose>0.06 -5.74878e-17 0 0 -0 0</pose>
                <mass>0.0628319</mass>
                <inertia>
                    <ixx>1.25664e-05</ixx>
                    <ixy>2.38766e-21</ixy>
                    <ixz>3.67013e-21</ixz>
                    <iyy>1.93732e-05</iyy>
                    <iyz>7.49977e-22</iyz>
                    <izz>1.93732e-05</izz>
                </inertia>
            </inertial>
            <collision name="link_collision">
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate_with_muscle/cad/sphere.STL</uri>
                    </mesh>
                </geometry>
            </collision>
            <visual name="link_visual">
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate_with_muscle/cad/sphere.STL</uri>
                    </mesh>
                </geometry>
            </visual>
        </link>

        <joint name="joint" type="revolute">
            <child>sphere</child>
            <parent>plate</parent>
            <axis>
                <xyz>0 -1 0</xyz>
                <limit>
                    <lower>-1.5</lower>
                    <upper>1.5</upper>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
                <use_parent_model_frame>1</use_parent_model_frame>
            </axis>
        </joint>

        <plugin filename="libroboySim.so" name="roboySim">
            <myoMuscle name="motor0">
                <link name="plate">
                    <viaPoint type="FIXPOINT">0.072174 0.000030 0.512747</viaPoint>
                </link>
                <link name="sphere">
                    <viaPoint type="FIXPOINT" state=1 revCounter=0 radius=0.02>0.0605 0.0 0.0251</viaPoint>
                </link>
                <link name="plate">
                    <viaPoint type="FIXPOINT">-0.068853 -0.000565 0.516309</viaPoint>
                </link>
            </myoMuscle>
        </plugin>
    </model>
</sdf>
